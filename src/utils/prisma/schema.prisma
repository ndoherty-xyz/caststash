generator client {
  provider        = "prisma-client-js"
  output          = "./generated"
  previewFeatures = ["views"]
}

generator kysely {
  provider        = "prisma-kysely"
  output          = "./kysely"
  previewFeatures = ["views"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model test_table { 
    id Int @default(autoincrement()) @id
}